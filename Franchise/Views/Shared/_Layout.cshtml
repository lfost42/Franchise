<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>@ViewData["Title"]Franchise Analytics</title>

    <meta property="og:image" content="https://lfost42-franchise.herokuapp.com/images/app.png" />
    <meta property="og:image:alt" content="Franchise Analytics Logo" />
    <meta name="twitter:card" content="summary_large_image">

    <meta property="og:title" content="Franchise Analytics" />
    <meta property="og:site_name" content="by Lynda Foster" />
    <meta property="og:type" content="article" />
    <meta name="twitter:creator" content="&#64;lfost42" />
    <link rel="icon" type="image/x-icon" href="https://lfost42-franchise.herokuapp.com/images/favicon.ico">

    <!--bootstrap cdn-->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

    <!--custom style-->
    <link rel="stylesheet" href="~/css/normalize.css" />
    <link rel="stylesheet" href="~/css/main.css" />
    <link rel="stylesheet" href="~/css/result.css" />

    <!--fontawesome cdn-->
    <script src="https://use.fontawesome.com/releases/v5.15.4/js/all.js" crossorigin="anonymous"></script>

</head>

<body class="homepage is-preload">

    <div id="page-wrapper">
        <main class="flex-shrink-0">
            @RenderBody()
        </main>

        <!-- Footer Section -->
        <div id="footer-wrapper">
            <section id="footer">
                <div class="container">
                    <ul class="copyright">
                        &copy;2021 Lynda Foster
                    </ul>

                    <ul class="icons">
                        <li>
                            <a href="https://github.com/lfost42"><i class="icon fab fa-github fa-lg"></i>
                            </a>
                        </li>
                        <li>
                            <a href="https://www.twitter.com/lfost42">
                                <i class="icon brands fab fa-twitter fs-4"></i>
                            </a>
                        </li>
                        <li>
                            <a href="https://www.reddit.com/user/lynda_">
                                <i class="icon fab fa-reddit-alien fa-lg"></i>
                            </a>
                        </li>
                        <li>
                            <a href="https://www.linkedin.com/in/lynda-foster/">
                                <i class="icon fab fa-linkedin fa-lg"></i>
                            </a>
                        </li>
                    </ul>
                </div>
            </section>
        </div>


    </div>
</body>
</html>

<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
@await RenderSectionAsync("Scripts", required: false)
